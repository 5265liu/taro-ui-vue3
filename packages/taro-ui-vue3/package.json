{
  "name": "taro-ui-vue3",
  "version": "0.0.1",
  "description": "Taro UI Rewritten in Vue 3.0",
  "author": "b2nil",
  "license": "MIT",
  "browser": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "types": "./types/index.d.ts",
  "keywords": [
    "taro",
    "taro-ui-vue3",
    "ui"
  ],
  "files": [
    "lib",
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:b2nil/taro-ui-vue3.git"
  },
  "templateInfo": {
    "name": "vuex",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "dev": "yarn dev:lib",
    "dev:lib": "tsc --project ./tsconfig.build.json --watch --incremental",
    "build": "yarn build:rollup && yarn build:lib",
    "build:lib": "tsc --project ./tsconfig.build.json",
    "build:lib-dev": "cross-env LIB_ENV=dev rollup --config ./config/rollup.config.js",
    "build:rollup": "rollup --config ./config/rollup.config.js",
    "test:analyze": "rollup --config ./config/rollup.config.analyze.js",
    "rollup:test": "node tests/transform-component.js && rollup --config ./config/rollup-test.config.js",
    "clean": "rimraf dist lib",
    "clog": "conventional-changelog -p angular -i CHANGELOG.md -s -r",
    "test": "node ./tests/transform-component.js && cross-env NODE_ENV=test && jest --coverage"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "dayjs": "^1.8.29",
    "lodash": "^4.17.19",
    "vue": "^3.0.0-rc.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/runtime": "^7.7.7",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@tarojs/components": "^3.0.7",
    "@tarojs/mini-runner": "3.0.7",
    "@tarojs/runtime": "3.0.7",
    "@tarojs/taro": "^3.0.7",
    "@tarojs/webpack-runner": "3.0.7",
    "@types/webpack-env": "^1.13.6",
    "@vue/compiler-sfc": "^3.0.0-rc.5",
    "babel-preset-taro": "^3.0.7",
    "csstype": "^2.6.11",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-visualizer": "^4.0.4",
    "rollup-plugin-vue": "^6.0.0-beta.8",
    "typescript": "^3.7.0",
    "vue-loader": "^16.0.0-beta.3"
  },
  "peerDependencies": {
    "@tarojs/components": "^3",
    "vue": "^3.0.0-rc.5"
  }
}
