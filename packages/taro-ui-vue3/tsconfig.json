{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": ["es6", "dom", "es2017"],
    "baseUrl": "./",
    "rootDir": "src",
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strictFunctionTypes": false,
    "strictNullChecks": false,
    "typeRoots": [
      "node",
      "node_modules/@types",
      "types",
      "global.d.ts"
    ],
    "paths": {
      "@/components/*": ["src/components/*"],
      "@/utils/*": ["src/utils/*"],
      "@/style/*": ["src/style/*"],
    }
  }
}